{
  "name": "react-redux-webpack-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build:development",
    "build:development": "npm run clean && npm run copy && npm run setup:development && webpack --mode development",
    "build:staging": "npm run clean && npm run copy && npm run setup:staging && webpack --config webpack.config.staging.js --mode development",
    "build:production": "npm run clean && npm run copy && npm run setup:production && webpack --config webpack.config.production.js --mode production",
    "clean": "npm run clean:www",
    "clean:www": "rimraf www/*",
    "copy": "npm run copy:html && npm run copy:css",
    "copy:html": "mkdir -p www && cp -r src/index.html www/",
    "copy:css": "mkdir -p www/assets/css && cp -r src/css/* www/assets/css/",
    "setup:development": "npm run setup:config:development",
    "setup:config:development": "/bin/bash -c 'if [[ ! -f src/js/config/development.js ]]; then cp src/js/config/default.js src/js/config/development.js; fi'",
    "setup:staging": "npm run setup:config:staging",
    "setup:config:staging": "/bin/bash -c 'if [[ ! -f src/js/config/staging.js ]]; then cp src/js/config/default.js src/js/config/staging.js; fi'",
    "setup:production": "npm run setup:config:production",
    "setup:config:production": "if [[ ! -f src/js/config/production.js ]]; then cp src/js/config/default.js src/js/config/production.js; fi",
    "watch": "npm run watch:html & npm run watch:webpack & npm run watch:browser",
    "watch:html": "cpx --watch src/index.html www",
    "watch:webpack": "webpack --watch -d",
    "watch:browser": "browser-sync start --config bs-config.js",
    "test": "npm run lint && npm run jest",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "`npm bin`/eslint ./src; exit 0;",
    "stylelint": "`npm bin`/stylelint \"./src/**/*.css\"",
    "jest": "jest --verbose --coverage",
    "tdd": "npm run jest -- --watch",
    "precommit": "`npm bin`/eslint ./src && npm run stylelint",
    "prepush": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nextremer/react-redux-webpack-template.git"
  },
  "keywords": [],
  "author": "k.tada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nextremer/react-redux-webpack-template/issues"
  },
  "homepage": "https://github.com/Nextremer/react-redux-webpack-template#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "moducks": "^1.0.0-beta.2",
    "moment": "^2.22.1",
    "react": "^16.3.2",
    "react-css-modules": "^4.7.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import-glob": "^2.0.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "^2.23.6",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "jest": "^22.4.3",
    "postcss-color-hex-alpha": "^3.0.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.4",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-node-externals": "^1.7.2"
  }
}
